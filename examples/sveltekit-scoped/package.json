{
  "name": "unocss-sveltekit",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "stackblitz:dev": "node remove-links.js && npm install && vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@iconify-json/logos": "^1.1.21",
    "@iconify-json/ri": "^1.1.4",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@unocss/core": "link:../../packages/core",
    "@unocss/preset-icons": "link:../../packages/preset-icons",
    "svelte": "^3.55.0",
    "svelte-check": "^3.0.1",
    "svelte-preprocess": "^5.0.0",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "unocss": "link:../../packages/unocss",
    "vite": "^4.0.4"
  },
  "stackblitz": {
    "installDependencies": false,
    "startCommand": "npm run stackblitz:dev"
  }
}
