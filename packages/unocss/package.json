{
  "name": "unocss",
  "version": "0.48.0",
  "description": "The instant on-demand Atomic CSS engine.",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/unocss/unocss#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unocss/unocss.git"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/antfu"
  },
  "bugs": {
    "url": "https://github.com/unocss/unocss/issues"
  },
  "keywords": [
    "unocss",
    "atomic-css",
    "atomic-css-engine",
    "css",
    "tailwind",
    "windicss"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./preset-attributify": {
      "types": "./dist/preset-attributify.d.ts",
      "require": "./dist/preset-attributify.cjs",
      "import": "./dist/preset-attributify.mjs"
    },
    "./preset-tagify": {
      "types": "./dist/preset-tagify.d.ts",
      "require": "./dist/preset-tagify.cjs",
      "import": "./dist/preset-tagify.mjs"
    },
    "./preset-icons": {
      "types": "./dist/preset-icons.d.ts",
      "require": "./dist/preset-icons.cjs",
      "import": "./dist/preset-icons.mjs"
    },
    "./preset-mini": {
      "types": "./dist/preset-mini.d.ts",
      "require": "./dist/preset-mini.cjs",
      "import": "./dist/preset-mini.mjs"
    },
    "./preset-typography": {
      "types": "./dist/preset-typography.d.ts",
      "require": "./dist/preset-typography.cjs",
      "import": "./dist/preset-typography.mjs"
    },
    "./preset-uno": {
      "types": "./dist/preset-uno.d.ts",
      "require": "./dist/preset-uno.cjs",
      "import": "./dist/preset-uno.mjs"
    },
    "./preset-web-fonts": {
      "types": "./dist/preset-web-fonts.d.ts",
      "require": "./dist/preset-web-fonts.cjs",
      "import": "./dist/preset-web-fonts.mjs"
    },
    "./preset-wind": {
      "types": "./dist/preset-wind.d.ts",
      "require": "./dist/preset-wind.cjs",
      "import": "./dist/preset-wind.mjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "require": "./dist/vite.cjs",
      "import": "./dist/vite.mjs"
    },
    "./astro": {
      "types": "./dist/astro.d.ts",
      "require": "./dist/astro.cjs",
      "import": "./dist/astro.mjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "require": "./dist/webpack.cjs",
      "import": "./dist/webpack.mjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub"
  },
  "peerDependencies": {
    "@unocss/webpack": "workspace:*"
  },
  "peerDependenciesMeta": {
    "@unocss/webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "@unocss/astro": "workspace:*",
    "@unocss/cli": "workspace:*",
    "@unocss/core": "workspace:*",
    "@unocss/preset-attributify": "workspace:*",
    "@unocss/preset-icons": "workspace:*",
    "@unocss/preset-mini": "workspace:*",
    "@unocss/preset-tagify": "workspace:*",
    "@unocss/preset-typography": "workspace:*",
    "@unocss/preset-uno": "workspace:*",
    "@unocss/preset-web-fonts": "workspace:*",
    "@unocss/preset-wind": "workspace:*",
    "@unocss/reset": "workspace:*",
    "@unocss/transformer-attributify-jsx": "workspace:*",
    "@unocss/transformer-compile-class": "workspace:*",
    "@unocss/transformer-directives": "workspace:*",
    "@unocss/transformer-variant-group": "workspace:*",
    "@unocss/vite": "workspace:*"
  },
  "devDependencies": {
    "@unocss/webpack": "workspace:*"
  }
}
